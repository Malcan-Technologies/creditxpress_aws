    "nodemon": "^3.1.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "node dist/prisma/seed.js || ts-node --project prisma/tsconfig.json prisma/seed.ts"
  }
}
