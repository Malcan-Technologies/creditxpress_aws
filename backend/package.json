{
  "name": "kapital-backend",
  "version": "1.0.0",
  "description": "Backend for Kapital application",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run prisma:generate && tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node --project prisma/tsconfig.json prisma/seed.ts",
    "prisma:fix": "bash scripts/fix-prisma-client.sh",
    "prisma:verify": "node scripts/verify-prisma-client.js",
    "generate-ctos-jwt": "ts-node scripts/generate-ctos-jwt.ts",
    "tsc": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.968.0",
    "@aws-sdk/s3-request-presigner": "^3.968.0",
    "@prisma/client": "^5.22.0",
    "@react-pdf/renderer": "^3.4.5",
    "@types/bcrypt": "^5.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "fast-xml-parser": "^5.3.2",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.12.9",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.15.0",
    "resend": "^4.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^1.0.35",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.111",
